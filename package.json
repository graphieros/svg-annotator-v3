  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "sass": "^1.57.1",
    "vite": "^4.0.0"
  }
}
