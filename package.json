{
  "name": "svg-annotator-v3",
  "description": "A Vue3 svg annotator to comment and print any wrapped content",
  "author": {
    "name": "Alec Lloyd Probert"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": { 
    "type": "git", 
    "url": "https://github.com/graphieros/svg-annotator-v3"
  }, 
  "license": "MIT",
  "keywords": ["annotator", "vue 3", "svg annotator", "svg"],
  "main": "./dist/svg-annotator-v3.umd.cjs",
